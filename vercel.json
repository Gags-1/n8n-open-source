{
  "version": 2,
  "builds": [
    {
      "src": "app/main.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9",
        "includeFiles": ["app/**"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app/main.py",
      "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"]
    }
  ],
  "env": {
    "PYTHONUNBUFFERED": "1",
    "NO_PROXY": "*",
    "HTTP_PROXY": "",
    "HTTPS_PROXY": ""
  }
}