{
  "version": 2,
  "builds": [
    {
      "src": "app/main.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9",
        "suppressProxyWarning": true
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app/main.py"
    }
  ],
  "env": {
    "PYTHONUNBUFFERED": "1",
    "NO_PROXY": "*",
    "HTTP_PROXY": "",
    "HTTPS_PROXY": "",
    "OPENAI_API_DISABLE_PROXY": "true"
  }
}